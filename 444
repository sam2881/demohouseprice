-- Assuming the destination table has already been created as follows:
-- CREATE TABLE parsed_data (
--     insertrecordid INT,
--     mpp VARCHAR(255),
--     ucid VARCHAR(255),
--     rtpvclost INT,
--     originatingnumber VARCHAR(255),
--     videobitrate INT,
--     redirectnumber VARCHAR(255),
--     id INT,
--     enddetails VARCHAR(255),
--     callstartdate DATE,
--     calltimestamp TIMESTAMP,
--     destinationnumber VARCHAR(255),
--     callstarttime INT,
--     areacode INT,
--     recordid INT,
--     rfroundtriptime INT,
--     sessionid VARCHAR(255),
--     teleprotocol VARCHAR(255),
--     mediatype INT,
--     videoscreensize INT,
--     videocodec INT,
--     receipttimestamp TIMESTAMP,
--     providername VARCHAR(255),
--     licenseplatequested VARCHAR(255),
--     zoneid INT,
--     portid INT,
--     insertdate DATE,
--     emlentryid INT,
--     insertid INT,
--     rtpvcjitter INT,
--     providermessage VARCHAR(255),
--     rtpsendreports INT,
--     telemediaencryption VARCHAR(255),
--     applicationname VARCHAR(255),
--     calltype INT,
--     rtpsendjitter INT,
--     created_datetime TIMESTAMP,
--     callid VARCHAR(255)
-- );

INSERT INTO parsed_data (
    insertrecordid, mpp, ucid, rtpvclost, originatingnumber, videobitrate, redirectnumber, id,
    enddetails, callstartdate, calltimestamp, destinationnumber, callstarttime, areacode, recordid,
    rfroundtriptime, sessionid, teleprotocol, mediatype, videoscreensize, videocodec, receipttimestamp,
    providername, licenseplatequested, zoneid, portid, insertdate, emlentryid, insertid, rtpvcjitter,
    providermessage, rtpsendreports, telemediaencryption, applicationname, calltype, rtpsendjitter,
    created_datetime, callid
)
SELECT
    JSON_VALUE(json_column, '$.INSERTRECORDID') AS insertrecordid,
    JSON_VALUE(json_column, '$.MPP') AS mpp,
    JSON_VALUE(json_column, '$.UCID') AS ucid,
    JSON_VALUE(json_column, '$.RTPVCLOST') AS rtpvclost,
    JSON_VALUE(json_column, '$.ORIGINATINGNUMBER') AS originatingnumber,
    JSON_VALUE(json_column, '$.VIDEOBITRATE') AS videobitrate,
    JSON_VALUE(json_column, '$.REDIRECTNUMBER') AS redirectnumber,
    JSON_VALUE(json_column, '$.ID') AS id,
    JSON_VALUE(json_column, '$.ENDDETAILS') AS enddetails,
    JSON_VALUE(json_column, '$.CALLSTARTDATE') AS callstartdate,
    JSON_VALUE(json_column, '$.CALLTIMESTAMP') AS calltimestamp,
    JSON_VALUE(json_column, '$.DESTINATIONNUMBER') AS destinationnumber,
    JSON_VALUE(json_column, '$.CALLSTARTTIME') AS callstarttime,
    JSON_VALUE(json_column, '$.AREACODE') AS areacode,
    JSON_VALUE(json_column, '$.RECORDID') AS recordid,
    JSON_VALUE(json_column, '$.RFROUNDTRIPTIME') AS rfroundtriptime,
    JSON_VALUE(json_column, '$.SESSIONID') AS sessionid,
    JSON_VALUE(json_column, '$.TELEPROTOCOL') AS teleprotocol,
    JSON_VALUE(json_column, '$.MEDIATYPE') AS mediatype,
    JSON_VALUE(json_column, '$.VIDEOSCREENSIZE') AS videoscreensize,
    JSON_VALUE(json_column, '$.VIDEOCODEC') AS videocodec,
    JSON_VALUE(json_column, '$.RECEIPTTIMESTAMP') AS receipttimestamp,
    JSON_VALUE(json_column, '$.PROVIDERNAME') AS providername,
    JSON_VALUE(json_column, '$.LICENSEPLATEQUESTED') AS licenseplatequested,
    JSON_VALUE(json_column, '$.ZONEID') AS zoneid,
    JSON_VALUE(json_column, '$.PORTID') AS portid,
    JSON_VALUE(json_column, '$.INSERTDATE') AS insertdate,
    JSON_VALUE(json_column, '$.EMLENTRYID') AS emlentryid,
    JSON_VALUE(json_column, '$.INSERTID') AS insertid,
    JSON_VALUE(json_column, '$.RTPVCJITTER') AS rtpvcjitter,
    JSON_VALUE(json_column, '$.PROVIDERMESSAGE') AS providermessage,
    JSON_VALUE(json_column, '$.RTPSENDREPORTS') AS rtpsendreports,
    JSON_VALUE(json_column, '$.TELEMEDIAENCRYPTION') AS telemediaencryption,
    JSON_VALUE(json_column, '$.APLICATIONNAME') AS applicationname,
    JSON_VALUE(json_column, '$.CALLTYPE') AS calltype,
    JSON_VALUE(json_column, '$.RTPSENDJITTER') AS rtpsendjitter,
    JSON_VALUE(json_column, '$.CREATED_DATETIME') AS created_datetime,
    JSON_VALUE(json_column, '$.CALLID') AS callid
FROM json_table;
